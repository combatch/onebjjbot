  "bugs": {
    "url": "https://github.com/combatch/onebjjbot/issues"
  },
  "homepage": "https://github.com/combatch/onebjjbot#readme",
  "dependencies": {
    "ap": "^0.2.0",
    "axios": "^0.18.0",
    "cross-env": "^5.1.1",
    "currency-formatter": "^1.3.1",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^6.0.0",
    "glossy": "^0.1.7",
    "graphql": "^0.13.2",
    "is-binary-path": "^2.0.0",
    "isomorphic-dompurify": "^0.15.0",
    "jimp": "^0.2.28",
    "jsonfile": "^4.0.0",

{
  "name": "onebjjbot",
  "version": "1.6.1",
  "description": "telegram bot written in node.js for one jiu jitsu. async/await",
  "main": "onebjjbot.js",
  "scripts": {
    "dev": "nodemon onebjjbot.js --ignore tmp/",
    "test": "pm2 stop onebjjbot && git pull && npm run prod",
    "build": "babel onebjjbot.js -o app.js",
    "prod": "cross-env NODE_ENV=production pm2 start onebjjbot.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/combatch/onebjjbot.git"
  },
  "keywords": [
    "telegram",
    "node",
    "es6",
    "async/await"
  ],
  "author": "",
